{% extends '@bolt/_partials/fields/_base.html.twig' %}

{% block field %}
    <editor-slug
        :value='{{ value|json_encode }}'
        :name='{{ name|json_encode }}'
        :prefix='{{ field.slugPrefix|json_encode }}'
        :field-class='{{ class|json_encode }}'
        :generate='{{ field.slugUseFields|join(', ')|json_encode }}'
    ></editor-slug>
{% endblock %}