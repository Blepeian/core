{% extends '@bolt/_partials/fields/_base.html.twig' %}

{% block field %}
   {% for fieldName, singleField in field.value %}
      {% set collectionItemName = 'collections[' ~ fieldName ~ ']' %}
      {% include '@bolt/_partials/fields/' ~ singleField.type ~ '.html.twig' with {'field': singleField, 'in_collection': true, 'name': collectionItemName } only %}
   {% endfor %}
{% endblock %}
