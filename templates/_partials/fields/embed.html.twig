{% extends '@bolt/_partials/fields/_base.html.twig' %}

{% block field %}
    <editor-embed
        :name="'{{ name }}'"
        :url="'{{ field.get('url') }}'"
        :width="'{{ field.get('width') }}'"
        :height="'{{ field.get('height') }}'"
        :title="'{{ field.get('title') }}'"
        :authorname="'{{ field.get('authorname') }}'"
        :author_url="'{{ field.get('author_url') }}'"
        :html="'{{ field.get('html') }}'"
        :thumbnail="'{{ field.get('thumbnail') }}'"
    ></editor-embed>
{% endblock %}
